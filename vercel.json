{
  "version": 2,
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/build",
  "installCommand": "cd backend && npm install && cd ..",
  "framework": null,
  "functions": {
    "api/index.ts": {
      "includeFiles": [
        "backend/src/**",
        "backend/api/**",
        "backend/package.json",
        "backend/node_modules/**"
      ],
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api"
    }
  ]
}


