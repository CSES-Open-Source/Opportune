{
  "version": 2,
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/build",
  "installCommand": "cd backend && npm install && cd ..",
  "framework": null,
  "functions": {
    "api/index.ts": {
      "includeFiles": "backend/src/**,backend/api/**,backend/package.json,backend/package-lock.json",
      "excludeFiles": "backend/node_modules/**,backend/dist/**,backend/seed-*.ts,backend/seed-*.js,backend/.git/**",
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api"
    }
  ]
}


