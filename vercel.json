{
  "version": 2,
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/build",
  "installCommand": "cd backend && HUSKY_SKIP_INSTALL=1 npm install --production && cd ..",
  "framework": null,
  "functions": {
    "api/index.ts": {
      "includeFiles": "backend/{dist/**,package.json,config/**}",
      "maxDuration": 30,
      "memory": 1052
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api"
    }
  ]
}